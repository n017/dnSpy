<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:baml="clr-namespace:dnSpy.BamlDecompiler"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:p="clr-namespace:dnSpy.BamlDecompiler.Properties;assembly=dnSpy.BamlDecompiler.Plugin">
	<DataTemplate DataType="{x:Type baml:BamlSettings}">
		<Grid Margin="5">
			<StackPanel>
                <CheckBox Margin="0 5 0 0" IsChecked="{Binding DisassembleBaml}" Content="{x:Static p:dnSpy_BamlDecompiler_Resources.BamlSettings_DisassembleBAML}" ToolTip="{x:Static p:dnSpy_BamlDecompiler_Resources.BamlSettings_DisassembleBAML_ToolTip}" />
			</StackPanel>
		</Grid>
	</DataTemplate>
</ResourceDictionary>
